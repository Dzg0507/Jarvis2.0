{
  "name": "jarvis-local",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm:dev:next\" \"npm:dev:mcp\"",
    "dev:next": "next dev",
    "dev:mcp": "npm run build:mcp && cross-env TS_NODE_PROJECT=tsconfig.mcp.json node -r tsconfig-paths/register dist-mcp/mcp-main.js",
    "build": "next build",
    "build:mcp": "tsc --project tsconfig.mcp.json",
    "start": "npm run build:mcp && concurrently \"npm:start:next\" \"npm:start:mcp\"",
    "start:next": "next start",
    "start:mcp": "cross-env TS_NODE_PROJECT=tsconfig.mcp.json node -r tsconfig-paths/register dist-mcp/mcp-main.js",
    "lint": "next lint",
    "test": "vitest src"
  },
  "dependencies": {
    "@google-cloud/text-to-speech": "^6.2.0",
    "@google/generative-ai": "^0.14.1",
    "@hookform/resolvers": "^5.2.1",
    "@modelcontextprotocol/sdk": "^1.17.4",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-toast": "^1.2.15",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.56.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cookies-next": "^4.2.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "elevenlabs": "^1.59.0",
    "express": "^4.19.2",
    "groq-sdk": "^0.30.0",
    "is-plain-object": "^5.0.0",
    "lucide-react": "^0.542.0",
    "marked": "^15.0.8",
    "next": "^14.2.32",
    "openai": "^5.16.0",
    "puppeteer": "^24.17.0",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "react": "^18",
    "react-dom": "^18",
    "react-hook-form": "^7.62.0",
    "selenium-webdriver": "^4.35.0",
    "tailwind-merge": "^3.3.1",
    "tw-animate-css": "^1.0.1",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/is-plain-object": "^0.0.3",
    "@types/marked": "^5.0.2",
    "@types/node": "^24.3.0",
    "@types/node-fetch": "^2.6.13",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/selenium-webdriver": "^4.1.22",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.0",
    "cross-env": "^10.0.0",
    "esbuild": "^0.25.9",
    "eslint": "^8",
    "eslint-config-next": "14.2.3",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.5.3",
    "vitest": "^3.2.4",
    "wait-on": "^8.0.4"
  }
}
